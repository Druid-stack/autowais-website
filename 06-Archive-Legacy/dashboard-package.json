{
  "name": "articshorex-dashboard",
  "version": "1.0.0",
  "description": "Real-time AI Agent Monitoring Dashboard for ArticShorex",
  "main": "dashboard-api.js",
  "scripts": {
    "start": "node dashboard-api.js",
    "dev": "nodemon dashboard-api.js",
    "test": "jest",
    "build": "echo 'No build step required for this project'",
    "lint": "eslint *.js",
    "setup": "npm install && echo 'Dashboard setup complete!'",
    "seed": "node scripts/seed-database.js"
  },
  "keywords": [
    "dashboard",
    "ai-agent",
    "monitoring",
    "real-time",
    "analytics",
    "customer-support",
    "n8n",
    "articshorex"
  ],
  "author": "ArticShorex Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mongodb": "^6.3.0",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "nodemailer": "^6.9.7",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/articshorex/ai-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/articshorex/ai-dashboard/issues"
  },
  "homepage": "https://github.com/articshorex/ai-dashboard#readme",
  "nodemonConfig": {
    "ignore": ["*.test.js", "*.spec.js"],
    "ext": "js,json,html",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!node_modules/**",
      "!coverage/**"
    ]
  }
} 