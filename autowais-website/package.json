{
  "name": "autowais-website",
  "version": "1.0.0",
  "description": "AUTOWAIS - Complete Website with Next.js Frontend and Node.js Backend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next",
    "dev:clean": "npm run clean && npm run dev",
    "analyze": "cross-env ANALYZE=true npm run build",
    "export": "next build && next export",
    "server": "node server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "setup": "node utils/setupAdmin.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:api": "jest --testPathPattern=api",
    "test:integration": "jest --testPathPattern=integration"
  },
  "dependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/nodemailer": "^6.4.17",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "next": "latest",
    "nodemailer": "^7.0.5",
    "postcss": "latest",
    "react": "latest",
    "react-dom": "latest",
    "tailwindcss": "^3.4.17",
    "typescript": "latest",
    "express": "^5.1.0",
    "mongoose": "^8.16.3",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "express-rate-limit": "^7.5.1",
    "dotenv": "^17.2.0",
    "morgan": "^1.10.0",
    "compression": "^1.8.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "axios": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@playwright/test": "^1.40.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.3",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.0"
  },
  "keywords": [
    "autowais",
    "ai",
    "automation",
    "saas",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "AUTOWAIS",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/autowais/autowais-website.git"
  },
  "bugs": {
    "url": "https://github.com/autowais/autowais-website/issues"
  },
  "homepage": "https://autowais.com"
}
