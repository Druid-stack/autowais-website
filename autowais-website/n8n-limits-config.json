{
  "executions": {
    "timeout": 3600,
    "maxTimeout": 3600,
    "saveDataOnError": "all",
    "saveDataOnSuccess": "all",
    "saveDataManualExecutions": true
  },
  "nodes": {
    "maxDataSize": "50MB",
    "binaryDataMode": "filesystem",
    "defaultMaxRetries": 3
  },
  "workflows": {
    "defaultName": "My Workflow",
    "onlySaveManualExecutions": false,
    "callerPolicyDefaultOption": "workflowsFromSameOwner"
  },
  "credentials": {
    "overwrite": {
      "data": "{\"host\":\"localhost\",\"port\":5432,\"database\":\"n8n\",\"username\":\"n8n\",\"password\":\"\"}"
    }
  },
  "generic": {
    "timezone": "America/New_York"
  },
  "logs": {
    "level": "info",
    "output": "console,file"
  },
  "queue": {
    "bull": {
      "redis": {
        "host": "localhost",
        "port": 6379,
        "password": "",
        "timeoutThreshold": 10000
      }
    }
  },
  "endpoints": {
    "metrics": {
      "enable": true,
      "prefix": "n8n_",
      "includeDefaultMetrics": true,
      "includeWorkflowIdLabel": true,
      "includeNodeTypeLabel": true,
      "includeCredentialTypeLabel": false,
      "includeApiEndpoints": true,
      "includeApiPathLabel": true,
      "includeApiMethodLabel": true,
      "includeApiStatusCodeLabel": true
    }
  },
  "externalHookFiles": "",
  "nodes": {
    "exclude": "[]",
    "include": "[]"
